mixin breadcrumbs(titles)
    - titles = titles ? titles : [];

    .breadcrumbs
        ul
            -for(var i = 0 ; i<titles.length ; i++)
                -if(i==0)
                    li
                        a(href='#')=titles[i]
                -else
                    span >
                    li
                        a(href='#')=titles[i]